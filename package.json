{
  "name": "dr-auto-submit",
  "version": "1.3.5-0",
  "description": "A auto submission tool to help with Deep Racer league",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "bin": "./lib/index.js",
  "scripts": {
    "prebuild": "rimraf lib",
    "build": "tsc --declaration",
    "start": "ts-node src/index.ts",
    "prepublishOnly": "npm run build",
    "release:rc": "scripts/release-rc.sh",
    "release:patch": "scripts/release.sh patch",
    "release:minor": "scripts/release.sh minor",
    "release:major": "scripts/release.sh major"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dungviettran89/dr-auto-submitter.git"
  },
  "keywords": [
    "deepracer",
    "auto",
    "submit"
  ],
  "author": "Dung Viet Tran",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/dungviettran89/dr-auto-submitter/issues"
  },
  "homepage": "https://github.com/dungviettran89/dr-auto-submitter#readme",
  "dependencies": {
    "commander": "^6.2.1",
    "puppeteer": "^14.2.1"
  },
  "devDependencies": {
    "@types/node": "^14.14.34",
    "pkg": "^4.4.9",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "files": [
    "src/**",
    "lib/**",
    "README.md",
    "package.json",
    "package-lock.json"
  ]
}
